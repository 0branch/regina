ac_subst_vars='SHELL PATH_SEPARATOR PACKAGE_NAME PACKAGE_TARNAME PACKAGE_VERSION PACKAGE_STRING PACKAGE_BUGREPORT exec_prefix prefix program_transform_name bindir sbindir libexecdir datadir sysconfdir sharedstatedir localstatedir libdir includedir oldincludedir infodir mandir build_alias host_alias target_alias DEFS ECHO_C ECHO_N ECHO_T LIBS VERSION VERSION_NODOT VER_DATE VER_MAJOR VER_MINOR VER_SUPP CC CFLAGS LDFLAGS CPPFLAGS ac_ct_CC EXEEXT OBJEXT build build_cpu build_vendor build_os host host_cpu host_vendor host_os target target_cpu target_vendor target_os SYS_DEFS binarybitprefix libdir64 libdir32 bitflag GCI_CONVERT_HEADER LEX LEXLIB LEX_OUTPUT_ROOT RANLIB ac_ct_RANLIB LN_S SET_MAKE CPP EGREP LIBOBJS ALLOCA ALLOCA_SHO ALLOCA_TSO DEBUG DEBUGGING PURIFY EFENCE DLFCNINCDIR DLFCNLIBDIR GCI_SHO HAVE_GCI SHL_SCRIPT THREADING_COMPILE THREADING_LINK THREADSAFE_TARGET MH_MT_LIBS MT_FILE REGINAMULTITHREAD TRACEMEM FLISTS REXXSOCKET regutil_config rxcurses_config rexxtk_config rexxgd_config rexxcurl_config rexxsql_config rexxisam_config rxsock_config rexxdw_config MH_FUNC_LIBS BASE_BINARY BASE_INSTALL BOTHLIBS CC2O CEXTRA DYNAMIC_LDFLAGS DYN_COMP EEXTRA EXECISER_DEP GETOPT LD_RXLIB_A1 LD_RXLIB_A2 LD_RXLIB_B1 LD_RXLIB_B2 LIBEXE LIBFILE LIBFLAGS LIBLINK LINKSHL LIBPRE LIBPST LINKREG O2SAVE O2SHO OBJ OBJECTS OSAVE OTHER_INSTALLS REGINAEXP SAVE2O SHLFILE SHLIBS SHLPRE BUNDLE SHL_LD SHL_TARGETS STATICLIB STATIC_LDFLAGS TEST1EXP TEST1EXPORTS TEST2EXP TEST2EXPORTS USE_ABI RANLIB_DYNAMIC SHLPST MODPST O2TSAVE O2TSO STATICLD_SHO RPMTOPDIR MH_UNSIGNED_CHAR_SWITCH LTLIBOBJS'
ac_subst_files=''

