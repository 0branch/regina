osis64bit=no
bitflag=""
case "$target" in
   *hp-hpux*)
      ;;
   *ibm-aix*)
      rc=`lsconf -k | grep -c 64-bit`
      if test $rc -eq 1; then
         bitflag="64"
         osis64bit=yes
      fi
      ;;
   *dec-osf4*)
      ;;
   *dec-osf*)
      ;;
   *sequent-dynix*)
      ;;
   *solaris*)
      rc=`isainfo -v | grep -c 64-bit`
      if test $rc -eq 1; then
         bitflag="64"
         osis64bit=yes
      fi
      ;;
   sparc*sunos*)
      ;;
   *linux*)
      mach="`uname -m`"
      if test "$mach" = "x86_64" -o "$mach" = "ia86" -o "$mach" = "alpha" -o "$mach" = "ppc64"; then
         bitflag="64"
         osis64bit=yes
      fi
      ;;
   *nto-qnx*)
      ;;
   *qnx*)
      ;;
   *beos*)
      ;;
   *cygwin*)
      ;;
esac

# Check whether --enable-32bit or --disable-32bit was given.
if test "${enable_32bit+set}" = set; then
  enableval="$enable_32bit"
  bitflag32=yes
else
  bitflag32=no
fi;
# Check whether --enable-64bit or --disable-64bit was given.
if test "${enable_64bit+set}" = set; then
  enableval="$enable_64bit"
  bitflag64=yes
else
  bitflag64=no
fi;

if test "x$bitflag32" = "xyes" -a "x$bitflag64" = "xyes"; then
   { { echo "$as_me:$LINENO: error: --enable-32bit and --enable-64bit flags cannot both be specified." >&5
echo "$as_me: error: --enable-32bit and --enable-64bit flags cannot both be specified." >&2;}
   { (exit 1); exit 1; }; }
fi

if test "x$bitflag64" = "xyes" -a "$osis64bit" = "no"; then
   { { echo "$as_me:$LINENO: error: --enable-64bit cannot be specified on non-64bit OS." >&5
echo "$as_me: error: --enable-64bit cannot be specified on non-64bit OS." >&2;}
   { (exit 1); exit 1; }; }
fi

binarybitprefix=""
if test "x$bitflag32" = "xyes"; then
   bitflag="32"
   if test "$osis64bit" = "yes"; then
      binarybitprefix="32"
   fi
elif test "x$bitflag64" = "xyes"; then
   bitflag="64"
fi


if test "$ac_cv_prog_CC" = "gcc" -o "$ac_cv_prog_CC" = "g++"; then
   if test "$bitflag" = "32"; then
      CFLAGS="${CFLAGS} -m32"
      LDFLAGS="${LDFLAGS} -m32"
   elif test "$bitflag" = "64"; then
      CFLAGS="${CFLAGS} -m64"
      LDFLAGS="${LDFLAGS} -m64"
   else
      bitflag="32"
   fi
elif test "$ac_cv_prog_CC" = "xlc" -o "$ac_cv_prog_CC" = "xlC"; then
   if test "$bitflag" = "32"; then
      CFLAGS="${CFLAGS} -q32"
      LDFLAGS="${LDFLAGS} -q32"
   elif test "$bitflag" = "64"; then
      CFLAGS="${CFLAGS} -q64"
      LDFLAGS="${LDFLAGS} -q64"
   else
      bitflag="32"
   fi
fi

if test "$libdir" = '${exec_prefix}/lib'; then
   if test "$osis64bit" = "yes"; then
      if test -d "/usr/lib64"; then
         libdir64='${exec_prefix}/lib64'
         libdir32='${exec_prefix}/lib'
      elif test -d "/usr/lib32"; then
         libdir32='${exec_prefix}/lib32'
         libdir64='${exec_prefix}/lib'
      else
         libdir64='${exec_prefix}/lib64'
         libdir32='${exec_prefix}/lib'
      fi
   else
      libdir64='${exec_prefix}/lib'
      libdir32='${exec_prefix}/lib'
   fi
   if test "$bitflag" = "64"; then
      libdir="$libdir64"
   else
      libdir="$libdir32"
   fi
else
   libdir64="$libdir"
   libdir32="$libdir"
fi







