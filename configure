   if test "$with_rxsock" = "yes"; then
      myfuncs="$myfuncs rxsock"
   fi
   if test "$with_rexxdw" = "yes"; then
      myfuncs="$myfuncs rexxdw"
   fi
   echo "          Static Loaded Packages: $myfuncs"
